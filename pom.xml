            <artifactId>maven-compiler-plugin</artifactId>
            <version>3.8.1</version>
            <configuration>
            <fork>true</fork> 
                 <executable>C:\Program Files\Java\jdk1.8.0_131\bin\javac.exe</executable>
		     <testSourceDirectory>/src/test/java</testSourceDirectory>
            </configuration>
        </plugin>
